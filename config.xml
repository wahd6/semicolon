        <icon   src="images/icon.png"   platform="windows"   width="120"  height="120" />
        <icon   src="images/icon.png"   platform="windows"   width="44"   height="44" />
        <icon   src="images/icon.png"   platform="windows"   width="106"  height="106" />
        <icon   src="images/icon.png"   platform="windows"   width="70"   height="70" />
        <icon   src="images/icon.png"   platform="windows"   width="71"   height="71" />
        <icon   src="images/icon.png"   platform="windows"   width="170"  height="170" />
        <icon   src="images/icon.png"   platform="windows"   width="360"  height="360" />
        <icon   src="images/icon.png"   platform="windows"   width="310"  height="310" />
        <icon   src="images/icon.png"   platform="windows"   width="310"  height="150" />
        <icon   src="images/icon.png"   platform="windows"   width="744"  height="360" />
        <splash src="images/icon.png"  	platform="windows"   width="1152" height="1920" />
	</platform>
	
    <!--
        Define access to external domains.
		
        <access />            - a blank access tag denies access to all external resources.
        <access origin="*" /> - a wildcard access tag allows access to all external resource.
		
        Otherwise, you can specify specific domains:
	-->
    <access origin="*" />
    <!--
		<access origin="http://phonegap.com" />                    - allow any secure requests to http://phonegap.com/
		<access origin="http://phonegap.com" subdomains="true" />  - same as above, but including subdomains, such as http://build.phonegap.com/
		<access origin="http://phonegap.com" browserOnly="true" /> - only allows http://phonegap.com to be opened by the child browser.
	-->
	
    <!-- Added the following intents to support the removal of whitelist code from base cordova to a plugin -->
    <!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
	
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
	</platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
	</platform>
	
</widget>
