<?xml version="1.0" encoding="UTF-8"?>

<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->
<widget xmlns     = "http://www.w3.org/ns/widgets"
xmlns:gap = "http://phonegap.com/ns/1.0"
id        = "com.snopzer.semicolon"
version   = "1.0.0">
	
    <name>Semicolon</name>
	
    <description>
        Semicolon Developers Interview Guide App. 
		I am Sure you will like This App Because it teaches all about interview skills
	</description>
	
    <author href="http://snopzer.com" email="info@snopzer.com">
        Mohammad Abdul Fareed
	</author>
	
    <!-- Define the main entry-point to the application -->
    <content src="index.html" />
	
    <!-- Customize your app and platform with the preference element. -->
    <preference name="DisallowOverscroll"         value="true" />
    <!-- android: MIN SDK version supported on the target device. MAX version is blank by default. -->
    <preference name="android-minSdkVersion"      value="28" />
	<preference name="AndroidPersistentFileLocation" value="Compatibility" />
	
    <!-- Define a specific version of PhoneGap to build into your app. -->
    <!-- <preference name="phonegap-version"       value="cli-6.0.0" /> -->
	
    <!-- Plugins -->
    <!-- Core plugins -->
    <plugin name="cordova-plugin-battery-status"      source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-camera"              source="npm" spec="~2.1.1" />
    <plugin name="cordova-plugin-media-capture"       source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-console"             source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-contacts"            source="npm" spec="~2.0.1" />
    <plugin name="cordova-plugin-device"              source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-device-motion"       source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-device-orientation"  source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-dialogs"             source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-file"                source="npm" spec="~4.1.1" />
    <plugin name="cordova-plugin-file-transfer"       source="npm" spec="~1.5.0" />
    <plugin name="cordova-plugin-geolocation"         source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-globalization"       source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-inappbrowser"        source="npm" spec="~1.3.0" />
    <plugin name="cordova-plugin-media"               source="npm" spec="~2.2.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen"        source="npm" spec="~3.2.1" />
    <plugin name="cordova-plugin-statusbar"           source="npm" spec="~2.1.2" />
    <plugin name="cordova-plugin-vibration"           source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist"           source="npm" spec="~1.2.1" />
	
    <!-- Define app icon and splashscreen for each platform. -->
    <platform name="android">
        <icon   src="images/icon.png"   density="ldpi" />
        <icon   src="images/icon.png"   density="mdpi" />
        <icon   src="images/icon.png"   density="hdpi" />
        <icon   src="images/icon.png"   density="xhdpi" />
        <icon   src="images/icon.png"   density="xxhdpi" />
        <icon   src="images/icon.png"   density="xxxhdpi" />
        <splash src="images/icon.png"   density="land-ldpi" />
        <splash src="images/icon.png"   density="land-mdpi" />
        <splash src="images/icon.png"   density="land-hdpi" />
        <splash src="images/icon.png"   density="land-xhdpi" />
        <splash src="images/icon.png"   density="land-xxhdpi" />
        <splash src="images/icon.png"   density="land-xxxhdpi" />
        <splash src="images/icon.png"   density="port-ldpi" />
        <splash src="images/icon.png"   density="port-mdpi" />
        <splash src="images/icon.png"   density="port-hdpi" />
        <splash src="images/icon.png"   density="port-xhdpi" />
        <splash src="images/icon.png"   density="port-xxhdpi" />
        <splash src="images/icon.png"   density="port-xxxhdpi" />
	</platform>
	
    <platform name="ios">
        <icon   src="images/icon.png"  platform="ios" width="57"   height="57" />
        <icon   src="images/icon.png"  platform="ios" width="114"  height="114" />
        <icon   src="images/icon.png"  platform="ios" width="40"   height="40" />
        <icon   src="images/icon.png"  platform="ios" width="80"   height="80" />
        <icon   src="images/icon.png"  platform="ios" width="50"   height="50" />
        <icon   src="images/icon.png"  platform="ios" width="100"  height="100" />
        <icon   src="images/icon.png"  platform="ios" width="60"   height="60" />
        <icon   src="images/icon.png"  platform="ios" width="120"  height="120" />
        <icon   src="images/icon.png"  platform="ios" width="180"  height="180" />
        <icon   src="images/icon.png"  platform="ios" width="72"   height="72" />
        <icon   src="images/icon.png"  platform="ios" width="144"  height="144" />
        <icon   src="images/icon.png"  platform="ios" width="76"   height="76" />
        <icon   src="images/icon.png"  platform="ios" width="152"  height="152" />
        <icon   src="images/icon.png"  platform="ios" width="29"   height="29" />
        <icon   src="images/icon.png"  platform="ios" width="58"   height="58" />
        <icon   src="images/icon.png"  platform="ios" width="87"   height="87" />
        <splash src="images/icon.png"  platform="ios" width="640"  height="1136" />
        <splash src="images/icon.png"  platform="ios" width="750"  height="1334" />
        <splash src="images/icon.png"  platform="ios" width="1242" height="2208" />
        <splash src="images/icon.png"  platform="ios" width="2208" height="1242" />
        <splash src="images/icon.png"  platform="ios" width="2048" height="1536" />
        <splash src="images/icon.png"  platform="ios" width="1024" height="768" />
        <splash src="images/icon.png"  platform="ios" width="1536" height="2048" />
        <splash src="images/icon.png"  platform="ios" width="768"  height="1024" />
        <splash src="images/icon.png"  platform="ios" width="640"  height="960" />
        <splash src="images/icon.png"  platform="ios" width="320"  height="480" />
	</platform>
	
    <platform name="wp8">
        <icon   src="images/icon.png"   platform="wp8" width="99"  height="99" />
        <icon   src="images/icon.png"        platform="wp8" width="159" height="159" />
        <splash src="images/icon.png" platform="wp8" width="768" height="1280" />
	</platform>
	
    <platform name="windows">
        <!-- For Windows the recommended approach to define app icons is to use target attribute -->
        <!-- <icon src="images/icon.png"         target="StoreLogo" /> -->
        <!-- <icon src="images/icon.png"   target="Square30x30Logo" /> -->
        <!-- <icon src="images/icon.png"   target="Square44x44Logo" /> -->
        <!-- <icon src="images/icon.png"   target="Square70x70Logo" /> -->
        <!-- <icon src="images/icon.png"   target="Square71x71Logo" /> -->
        <!-- <icon src="images/icon.png" target="Square150x150Logo" /> -->
        <!-- <icon src="images/icon.png" target="Square310x310Logo" /> -->
        <!-- <icon src="images/icon.png"   target="Wide310x150Logo" /> -->
        <icon src="images/icon.png"     platform="windows"   width="150"  height="150" />
        <icon src="images/icon.png"     platform="windows"   width="30"   height="30" />
        <icon src="images/icon.png"     platform="windows"   width="50"   height="50" />
        <splash src="images/icon.png"   platform="windows"   width="620"  height="300" />
